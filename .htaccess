RewriteEngine On
RewriteBase /
RewriteCond %{REQUEST_FILENAME}/index\.htm !-f
RewriteCond %{REQUEST_FILENAME}/index\.html !-f
RewriteCond %{REQUEST_FILENAME}/index\.shtml !-f
RewriteCond %{REQUEST_FILENAME}/index\.asp !-f
RewriteCond %{REQUEST_FILENAME}/index\.aspx !-f
RewriteCond %{REQUEST_FILENAME}/index\.php !-f
RewriteCond %{REQUEST_FILENAME}/index\.cgi !-f
RewriteCond %{REQUEST_FILENAME}/default\.htm !-f
RewriteCond %{REQUEST_FILENAME}/default\.html !-f
RewriteCond %{REQUEST_FILENAME}/default\.shtml !-f
RewriteCond %{REQUEST_FILENAME}/default\.asp !-f
RewriteCond %{REQUEST_FILENAME}/default\.aspx !-f
RewriteCond %{REQUEST_FILENAME}/default\.php !-f
RewriteCond %{REQUEST_FILENAME}/default\.cgi !-f
RewriteCond %{REQUEST_FILENAME} -d

# Path to Teepee from webroot
RewriteRule .* /resources/teepee/ [L]
